<h1>All Wikis <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success pull-right' %></h1>

<% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to wiki.title, wiki, onclick: "blocmetrics.report('wikiClick')" %>
      </h4>
    </div>
  </div>
<% end %>
